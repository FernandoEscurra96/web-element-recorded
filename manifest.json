{
  "manifest_version": 3,
  "name": "Selenium Recorder",
  "version": "1.1",
  "description": "Captura pasos de usuario y los env√≠a a un servidor para generar tests Selenium.",
  "permissions": ["scripting", "tabs", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
